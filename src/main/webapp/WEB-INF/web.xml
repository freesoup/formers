<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Formers Web Application</display-name>
	<servlet>
		<servlet-name>PrinterServlet</servlet-name>
		<servlet-class>formers.ui.servlet.Printer</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AdminDashBoard</servlet-name>
		<servlet-class>formers.ui.servlet.AdminDashboardServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FormSubmit</servlet-name>
		<servlet-class>formers.ui.servlet.FormSubmitServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FormView</servlet-name>
		<servlet-class>formers.ui.servlet.FormViewServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>PrinterServlet</servlet-name>
		<url-pattern>/cool</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AdminDashBoard</servlet-name>
		<url-pattern>/mainadmin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FormSubmit</servlet-name>
		<url-pattern>/submitform</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FormView</servlet-name>
		<url-pattern>/viewform</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>fontslogin.html</welcome-file>
	</welcome-file-list>
</web-app>
